{
  "version": 2,
  "name": "halakt-backend",
  "builds": [{
    "src": "dist/main.js"
  }],
  "scripts": {
    "build": "npm install && npm run build && npm run db:push"
  },
  "routes": [{
    "src": "/(.*)",
    "dest": "dist/main.js",
    "methods": [
      "GET",
      "POST",
      "PUT",
      "DELETE"
    ]
  }]
}